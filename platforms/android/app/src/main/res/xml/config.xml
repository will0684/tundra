<?xml version='1.0' encoding='utf-8'?>
<widget id="com.algonquin.will0684" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Tundra</name>
    <description>
        A dating app for citizens of the Tundra.
    </description>
    <author email="jordan.walcaraz@gmail.com" href="">
        Algonquin Mobile Application Design and Development
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon/android/tundra.ico" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
